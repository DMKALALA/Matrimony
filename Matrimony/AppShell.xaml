<Shell 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Matrimony.Views"
    x:Class="Matrimony.AppShell">

    <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Route="SignupPage" ContentTemplate="{DataTemplate views:SignupPage}" />
    <ShellContent Route="MainPage" ContentTemplate="{DataTemplate views:MainPage}" />
    <ShellContent Route="GuestPage" ContentTemplate="{DataTemplate views:GuestPage}" />

</Shell>
